# Environment Configuration Template
# Copy this file to .env and update the values

# Server Configuration
PORT=3001
NODE_ENV=development

# CORS Configuration
# Development: http://localhost:3000,http://localhost:5173
# Production: https://dcodesys.in,https://app.dcodesys.in
CORS_ORIGIN=http://localhost:3000,http://localhost:5173,https://dcodesys.in,https://app.dcodesys.in

# Rate Limiting
RATE_LIMIT_WINDOW=900000
RATE_LIMIT_MAX=100

# Debug Settings
DEBUG=true
LOG_LEVEL=debug

# Security Settings
TRUST_PROXY=false
HELMET_ENABLED=false

# Performance Settings
CACHE_ENABLED=false
CACHE_TTL=3600000

# YouTube Extraction Settings
YT_DLP_TIMEOUT=30000
YT_DLP_MAX_RETRIES=3
YT_DLP_USER_AGENT=Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36

# Judge0 Configuration
# Judge0 Cloud API (RapidAPI)
# Get your API key from: https://rapidapi.com/judge0-official/api/judge0-ce
JUDGE0_BASE_URL=https://judge0-ce.p.rapidapi.com
JUDGE0_API_KEY=your-rapidapi-key-here
# Alternative: You can also use JUDGE0_RAPIDAPI_KEY
# JUDGE0_RAPIDAPI_KEY=your-rapidapi-key-here
JUDGE0_TIMEOUT=20000
